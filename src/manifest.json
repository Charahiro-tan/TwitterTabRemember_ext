{
    "manifest_version": 3,
    "name": "TwitterTabRemember",
    "description": "Twitterのタブの位置を覚えます",
    "version": "0.0.2",
    "content_scripts": [
        {
            "matches": [
                "*://twitter.com/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}